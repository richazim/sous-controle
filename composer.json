            "SousControle\\Database\\": "database/"
        },
        "files": [
        "core/helpers/arrays.php",
        "core/helpers/exceptions.php",
        "core/helpers/files.php",
        "core/helpers/environment.php",
        "core/helpers/configurations.php"
        ]
    },
    "authors": [
        {
            "name": "richazim",
            "email": "saibouazim90@gmail.com"
        }
    ],
    "require-dev": {
        "symfony/var-dumper": "^7.2",
        "pestphp/pest": "^3.7"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
